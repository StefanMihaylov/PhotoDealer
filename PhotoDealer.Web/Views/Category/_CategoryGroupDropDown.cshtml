@model  IEnumerable<PhotoDealer.Web.ViewModels.CategoryDropDownViewModel>

@{
    var dropDownItems = new List<SelectListItem>();
    var newListItem = new SelectListItem() { Value = null, Text = "Any" };
    dropDownItems.Add(newListItem);

    foreach (var item in Model)
    {
        newListItem = new SelectListItem() { Value = item.Id.ToString(), Text = item.Name };
        dropDownItems.Add(newListItem);
    }
}


@Html.DropDownList("CategoryGroup", @dropDownItems, new { @class = "form-control" })
